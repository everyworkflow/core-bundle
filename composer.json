{
    "name": "everyworkflow/core-bundle",
    "description": "Core bundle for every workflow project.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "EveryWorkflow",
            "email": "everyworkflow@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/annotations": "^1.13",
        "doctrine/inflector": "^2.0",
        "opis/json-schema": "^2.0",
        "symfony/cache": "6.0.x",
        "symfony/uid": "6.0.x",
        "nelmio/cors-bundle": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "EveryWorkflow\\CoreBundle\\": "./"
        },
        "exclude-from-classmap": [
            "Resources",
            "Tests"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EveryWorkflow\\CoreBundle\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev"
}
